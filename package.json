{
  "name": "besharp-cdk-etl",
  "version": "0.1.0",
  "bin": {
    "besharp-cdk-etl": "infrastructure/bin/besharp-cdk-etl.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "synth:dev": "npm run build && ENVIRONMENT=dev cdk synth",
    "deploy:dev": "ENVIRONMENT=dev cdk deploy BesharpCdkPipelineStack",
    "deploy-infra:dev": "ENVIRONMENT=dev cdk deploy BesharpCdkInfraStack",
    "synth:prod": "ENVIRONMENT=prod cdk synth",
    "deploy:prod": "ENVIRONMENT=prod cdk deploy BesharpCdkPipelineStack",
    "deploy-infra:prod": "ENVIRONMENT=dev cdk deploy BesharpCdkInfraStack"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "10.17.27",
    "@types/prettier": "2.6.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "aws-cdk": "2.44.0",
    "ts-node": "^10.9.1",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "aws-cdk-lib": "2.44.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
